[
  {"selector":".html",
    "style":{
      "background-color":"#f5ecf4"}
  },

  {
    "selector": "node",
    "style": {
      "text-valign": "top",
      "text-halign": "center",
      "color": "black",
      "text-outline-width": 2,
      "text-outline-color": "white",
      "font-size": "20px",
      "opacity": 0.9,
      "shape": "hexagon",
      "background-image": "https://upload.wikimedia.org/wikipedia/commons/2/22/The_queen_of_sweden_roses.jpg",
      "width":100,
      "height": 90
    }
  },

  {
    "selector": "edge",
    "style": {
      "target-arrow-shape": "none",
      "line-color": "green",
      "curve-style": "bezier",
      "width": 2,
      "font-size": 13,
      "source-text-offset": 30,
      "target-text-offset": 40,
      "source-text-margin-x": 0,
      "source-text-margin-y": 12,
      "target-text-margin-x": 0,
      "target-text-margin-y": 12,
      "source-text-rotation": "autorotate",
      "target-text-rotation": "autorotate"
    }
  }
]
